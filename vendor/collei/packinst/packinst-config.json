{
	"target": "collei/plat",
	// disables install of certain packages
	"disable_add": {
		"list": [
			"laravel/*",
			"symfony/*",
			"composer/*"
		],
		"except": [
		]
	},
	// disables removal of certain packages
	"disable_removal": {
		"list": [
			"collei/collei",
			"collei/dately",
			"collei/packinst"
		],
		"except": [
		]
	}
}
